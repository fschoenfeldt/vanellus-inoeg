{
    "compilerOptions": {
        "target": "es2019",
        "lib": ["es5", "es6", "dom", "dom.iterable"],
        "module": "commonjs",
        "moduleResolution": "node",
        "baseUrl": "./",
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}
